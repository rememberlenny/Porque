<h1>New question</h1>

<form id="new-question" name="question">
  <div class="field">
    <label for="question"> question:</label>
    <input type="text" name="question" id="question" value="<%= question %>" >
  </div>

  <div class="field">
    <label for="media"> media:</label>
    <input type="text" name="media" id="media" value="<%= media %>" >
  </div>

  <div class="field">
    <label for="answerText"> answerText:</label>
    <input type="text" name="answerText" id="answerText" value="<%= answerText %>" >
  </div>

  <div class="field">
    <label for="state"> state:</label>
    <input type="text" name="state" id="state" value="<%= state %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Create Questions" />
  </div>

</form>

